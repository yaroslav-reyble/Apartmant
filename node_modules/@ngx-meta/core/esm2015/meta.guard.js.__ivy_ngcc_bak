import * as tslib_1 from "tslib";
import { Injectable } from '@angular/core';
import { MetaService } from './meta.service';
let MetaGuard = class MetaGuard {
    constructor(meta) {
        this.meta = meta;
    }
    canActivate(route, state) {
        const url = state.url;
        const metaSettings = route.hasOwnProperty('data') ? route.data.meta : undefined;
        this.meta.update(url, metaSettings);
        return true;
    }
    canActivateChild(route, state) {
        return this.canActivate(route, state);
    }
};
MetaGuard.ctorParameters = () => [
    { type: MetaService }
];
MetaGuard = tslib_1.__decorate([
    Injectable(),
    tslib_1.__metadata("design:paramtypes", [MetaService])
], MetaGuard);
export { MetaGuard };
